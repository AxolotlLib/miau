{
    "version": 2,
    "builds": [
      {
        "src": "api/index.ts",
        "use": "@vercel/node"
      },
      {
        "src": "client/(.*)",
        "use": "@astrojs/vercel"
      }
    ],
    "routes": [
      {
        "src": "api/(.*)",
        "dest": "api/index.ts" 
      }
    ]
  }